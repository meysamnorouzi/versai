# WordPress API Configuration
VITE_WORDPRESS_API_URL=https://your-wordpress-site.com/wp-json/wp/v2
VITE_WORDPRESS_SITE_URL=https://your-wordpress-site.com
VITE_WORDPRESS_API_KEY=your-api-key-here
VITE_WORDPRESS_USERNAME=your-username
VITE_WORDPRESS_PASSWORD=your-password

# Versai Custom API
VITE_VERSAI_API_URL=https://your-wordpress-site.com/wp-json/versai/v1

# Google Analytics
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_GOOGLE_TAG_MANAGER_ID=GTM-XXXXXXX

# Social Media
VITE_FACEBOOK_APP_ID=your-facebook-app-id
VITE_TWITTER_HANDLE=@versai_immigration

# Environment
VITE_NODE_ENV=development
VITE_APP_TITLE=موسسه مهاجرتی ورسای
VITE_APP_DESCRIPTION=اقامت تمکن مالی و تحصیلی فرانسه - کانادا

# API Settings
VITE_API_TIMEOUT=10000
VITE_API_RETRY_ATTEMPTS=3
VITE_CACHE_TIMEOUT=300000

# CORS Settings
VITE_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173,https://versai.ir

# Debug Settings
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=info
